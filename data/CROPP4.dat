RNFLV    = 0.0043   ! residual N fraction in leaves [kg N kg-1 dry biomass]
RNFST    = 0.0021   ! residual N fraction in stems [kg N kg-1 dry biomass]
RNFRT    = 0.0021   ! residual N fraction in roots [kg N kg-1 dry biomass]
