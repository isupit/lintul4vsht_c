            16.00,    0.00,      ! as function of day-time temp. [cel; -],
            18.00,    0.25,     ! values from SWHEAT model + 8C    
            24.00,    0.67,
            28.00,    0.83,
            33.00,    0.93,
            38.00,    1.00,
            43.00,    1.00


